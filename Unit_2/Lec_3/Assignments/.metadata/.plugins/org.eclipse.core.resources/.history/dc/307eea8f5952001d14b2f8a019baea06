#include <stdio.h>


int main() {
	float a,b;
	printf("Enter Value of a:");
	fflush(stdin); fflush(stdout);
	scanf("%f",&a);
	printf("Enter Value of b:");
	fflush(stdin); fflush(stdout);
	scanf("%f",&b);
	printf("After Swapping, value of a = %f\n", b);
	printf("After Swapping, value of b = %f\n", a);

	return 0;
}
