#include <stdio.h>

int main(void) {
	int x; int y;
	printf("Enter Two integers: ");
	fflush(stdin);fflush(stdout);
	scanf("%d %d" ,x ,y);
	printf("Sum: %d", x+y);

	return 0;
}
